VERSION 5.7 ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN test_4_sinks ;
UNITS DISTANCE MICRONS 2000 ;
DIEAREA ( 0 0 ) ( 10000000 10000000) ;

COMPONENTS 4 ;
- ff1 DFF_X1 + PLACED ( 2400000 2600000 ) N ;
- ff2 DFF_X1 + PLACED ( 8000000 2400000 ) N ;
- ff3 DFF_X1 + PLACED ( 2600000 7600000 ) N ;
- ff4 DFF_X1 + PLACED ( 7400000 6600000 ) N ;
END COMPONENTS

PINS 1 ;
- clk + NET clk + DIRECTION INPUT + USE SIGNAL 
  + LAYER metal6 ( -140 0 ) ( 140 280 ) + FIXED ( 10000 20000 ) S ;
END PINS

NETS 1 ;
- clk ( PIN clk ) ( ff1 CK ) ( ff2 CK )
      ( ff3 CK ) ( ff4 CK )
 ;
END NETS

END DESIGN